{
  "name": "vue-domain-name-extractor",
  "description": "A simple root domain name extractor from any kind of URL without subdomains",
  "version": "0.1.0",
  "private": false,
  "author":{
    "name" : "dev@arunya",
    "email": "dev.arunya@gmail.com"
  },
  "repository": "https://github.com/aru-pro123/vue-domain-name-extractor",
  "bugs": "https://github.com/aru-pro123/vue-domain-name-extractor/issues",
